<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:alpha="1"
    android:background="@drawable/backgroundmon"
    tools:context="com.example.harlemknights.TimeRush.AfterGameActivity">

    <Button
        android:id="@+id/MyMain"
        android:layout_width="127dp"
        android:layout_height="wrap_content"
        android:layout_x="136dp"
        android:layout_y="499dp"
        android:text="Main Menu" />

    <Button
        android:id="@+id/playAgainButton"
        android:layout_width="154dp"
        android:layout_height="wrap_content"
        android:layout_x="122dp"
        android:layout_y="413dp"
        android:text="Play Again" />

    <TextView
        android:id="@+id/Comment"
        android:layout_width="210dp"
        android:layout_height="44dp"
        android:layout_x="88dp"
        android:layout_y="197dp"
        android:textColor="#ffff00"
        android:textSize="20dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/ScoreView"
        android:layout_width="201dp"
        android:layout_height="74dp"
        android:layout_x="69dp"
        android:layout_y="69dp"
        android:textColor="#ffff00"
        android:textSize="25dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="79dp"
        tools:layout_editor_absoluteY="67dp" />

    <TextView
        android:id="@+id/HighScore"
        android:layout_width="195dp"
        android:layout_height="65dp"
        android:layout_x="91dp"
        android:layout_y="299dp"
        android:text="TextView"
        android:textSize="20dp" />
</AbsoluteLayout>
